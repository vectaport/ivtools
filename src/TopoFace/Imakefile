XCOMM
XCOMM TopoFace - topology library: nodes, edges, faces
XCOMM

PACKAGE = TopoFace

#ifdef InObjectCodeDir

LIB = TopoFace

Use_libUnidraw()
Use_2_6()

MakeLibrary($(LIB),$(VERSION))

#define Obj(file) MakeObjectFromSrcFlags(file,)

Obj(fgeomobjs)
Obj(topoedge)
Obj(topoedgelist)
Obj(topoelt)
Obj(topoface)
Obj(toponode)

IncludeDependencies()

#else

MakeInObjectCodeDir()
InstallIncludes($(PACKAGE))

#endif
