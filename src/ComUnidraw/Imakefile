PACKAGE = ComUnidraw

#ifdef InObjectCodeDir

LIB = ComUnidraw

Use_libUnidraw()
Use_2_6()

MakeLibrary($(LIB),$(VERSION))

#define Obj26(file) MakeObjectFromSrcFlags(file,-xc++)

Obj26(grfunc)
Obj26(comeditor)
Obj26(comterp-iohandler)
Obj26(unifunc)

#ifdef AceDir
#define ObjACE(file) MakeObjectFromSrcFlags(file,-xc++ -Uiv2_6_compatible -I$(TOP)/src/include $(TOP_CCINCLUDES) -D__ACE_INLINE__)
ObjACE(comterp-acehandler)
#endif

#define Obj31(file) MakeObjectFromSrcFlags(file,-xc++ -Uiv2_6_compatible -I$(TOP)/src/include $(TOP_CCINCLUDES))

IncludeDependencies()

#else

MakeInObjectCodeDir()

#endif
